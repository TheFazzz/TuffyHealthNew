@model IEnumerable<TuffyHealthNew.Models.PatientLogs>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Tests</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Patient_log_date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Treatments.TreatmentName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Result)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Patient_log_date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Treatments.TreatmentName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Result)
                </td>


            </tr>
        }
    </tbody>
</table>

